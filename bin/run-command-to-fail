#!/usr/bin/env perl

use strict;
use warnings;

use App::Run::Command::ToFail;

our $VERSION = 0.01;

# Run.
exit App::Run::Command::ToFail->new->run;

# Print version.
sub VERSION_MESSAGE {
	print $VERSION."\n";
	exit 0;
}

__END__

=pod

=encoding utf8

=head1 NAME

run-command-to-fail - Tool to run command in cycle to check fail.

=head1 SYNOPSIS

 run-command-to-fail [-h] [--version]

=head1 ARGUMENTS

=over 8

=item * C<-h>

Print help.

=item * C<--version>

Print version of script.

=back

=head1 EXAMPLE

 run-command-to-fail TODO

